{
  "name": "@stepfun/n8n-nodes-stepfun-integration",
  "version": "1.0.4",
  "description": "n8n community node: Stepfun.ai TTS (text-to-speech)",
  "license": "MIT",
  "author": "Owen Shen <owenshen123@outlook.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owenshen0907/n8n-node-stepfun.git"
  },
  "bugs": {

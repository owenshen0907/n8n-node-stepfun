{
  "name": "n8n-node-stepfun",
  "version": "0.1.0",
  "private": true,
  "description": "n8n community node: Stepfun.ai TTS (text-to-speech)",
  "license": "UNLICENSED",
  "homepage": "https://platform.stepfun.com/",
  "scripts": {
    "n8n-node-dev": "node ./scripts/n8n-node-dev.cjs",
    "build": "tsc -p tsconfig.json && node ./scripts/copy-assets.cjs",
    "build:watch": "tsc -p tsconfig.json --watch",
    "new": "npm run n8n-node-dev -- new"
  },
  "n8n": {
    "nodes": [
      "dist/nodes/StepFunTts/StepFunTts.node.js"
    ],
    "credentials": ["dist/credentials/StepFunApi.credentials.js"]
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "n8n-core": "1.122.12",
    "n8n-node-dev": "1.121.10",
    "n8n-workflow": "1.120.7",
    "typescript": "^5.9.3"
  }
}
